{
  "name": "simple-blog",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "dev:server": "node ./json-server/index.js",
    "build": "vite build",
    "analyze": "ANALYZE=true vite build",
    "preview": "vite preview --port=3000",
    "lint:ts": "eslint \"**/*.{ts,tsx,vue}\"",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx,vue}\" --fix",
    "lint:css": "stylelint \"src/**/*.{css,vue}\"",
    "lint:css:fix": "stylelint \"src/**/*.{css,vue}\" --fix",
    "test:unit": "vitest run --config ./config/vitest/vitest.config.ts",
    "test:ui": "LOCAL=true lost-pixel",
    "test:ui:ok": "LOCAL=true lost-pixel update",
    "test:ui:report": "npm run test:ui:json && npm run test:ui:html",
    "test:ui:json": "node scripts/generate-visual-json-report.js",
    "test:ui:html": "reg-cli --from .lostpixel/report.json --report .lostpixel/report.html",
    "storybook": "storybook dev --no-open -p 6006 -c ./config/storybook",
    "storybook:build": "storybook build -c ./config/storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/epchela/simple-blog.git"
  },
